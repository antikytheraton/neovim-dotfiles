{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "javascript",
    "typescript",
    "typescriptreact",
    "typescript.tsx"
  ],
  "python.pythonPath": "~/.config/nvim/python_path"
}
